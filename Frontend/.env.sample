# Frontend environment variables (sample)
# Copy this file to `.env` and fill values.

# Backend API base URL
VITE_BASE_URL=http://localhost:2804/api/v1

# ImageKit base URL (used for logo/video and favicon)
VITE_IMAGEKIT_URL=https://ik.imagekit.io/<your_imagekit_id>

# Optional: Google OAuth redirect URLs (if you use them in Auth page)
VITE_GOOGLE_URL1=http://localhost:2804/api/v1/alumni/google
VITE_GOOGLE_URL2=http://localhost:2804/api/v1/students/google
